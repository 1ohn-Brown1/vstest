{
  "version": "15.0.0-*",

  "buildOptions": {
    "delaySign": true,
    "keyFile": "../../scripts/key.snk",
    "warningsAsErrors": true
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0-rc2-3002702"
    },
    "dotnet-test-mstest": {
      "version": "1.0.1-preview",
      "exclude": "compile"
    },
    "MSTest.TestFramework": "1.0.0-preview",
    "Microsoft.TestPlatform.Utilities": "15.0.0-*"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  },

  "testRunner": "mstest"
}